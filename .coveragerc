[run]
source = turbodrf
omit = 
    */tests/*
    */test_*
    */migrations/*
    */venv/*
    */virtualenv/*
    */.venv/*
    */env/*
    */setup.py
    */conftest.py
    */manage.py
    */__pycache__/*
    */tox/*

[report]
precision = 2
show_missing = True
skip_covered = False
skip_empty = True

exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover
    
    # Don't complain about missing debug-only code:
    def __repr__
    if self\.debug
    
    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError
    
    # Don't complain if non-runnable code isn't run:
    if 0:
    if __name__ == .__main__.:
    
    # Don't complain about abstract methods
    @(abc\.)?abstractmethod
    
    # Type checking blocks
    if TYPE_CHECKING:
    
ignore_errors = True

[html]
directory = htmlcov

[xml]
output = coverage.xml